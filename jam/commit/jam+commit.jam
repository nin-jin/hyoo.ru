with( $jam )
$define
(   '$commit'
,   new function(){
        $Node( $doc().documentElement )
        .listen( 'keyup', function( ev ){
            if( !ev.ctrlKey || ev.keyCode != 13 ) return
            //$log(ev.charCode,ev.keyCode,ev.ctrlKey,ev.target,ev.srcElement,ev)
            $Node( ev.target ).scream({ type: '$jam.$commit' })
        })
    }
)
